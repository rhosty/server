{
  "version": 2,
  "builds": [
    {
      "src": "api/server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "todo/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/server.cjs"
    }
  ]
}
